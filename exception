package FILES;

public class Exception {
	public static void main(String args[]) {
		int a=5,b=0,c;
		try {
			int arg[]= {1,2,3,4,5,6};
			for( int i=0;i<6;i++)
			{
				System.out.println(arg[6]);
			}
		}
		catch(ArithmeticException e)
		{
			System.out.println("e");
		}
		catch(ArrayIndexOutOfBoundsException f)
		{
			System.out.println("f");
		}
	}

}
